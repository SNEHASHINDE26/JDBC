
package apna;
import java.util.*;
import java.util.Scanner;




public class alpha {

	public static void main(String[] args) {
		
//		Scanner sc = new Scanner(System.in);
		
		for(int line=1;line<=5;line++) {
			for(int num=1;num<=line;num++) {
				System.out.print(num);
			}
			System.out.println(" ");
		}
	}
}


