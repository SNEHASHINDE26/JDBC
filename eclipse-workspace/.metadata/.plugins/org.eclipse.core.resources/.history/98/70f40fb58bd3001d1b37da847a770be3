//pro 6 :
		
		int row=5;
	    int col=5;
	    for(int i=1;i<=row;i++) {
	    	for(int j=1;j<=col;j++) {
	    		System.out.print(i+" ");
	    	}
	    	System.out.println();
	    }
	    
