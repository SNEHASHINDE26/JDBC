package PatternProgramming;

public class day11 {

	public static void main(String[] args) {
		
		  int star=5;
		 int row=5;
		 int space=0;
		 
		 for(int i=1;1<=row;i++ ) {
			 for(int j=1;j<=star;j++) {
				  System.out.prin  t("* ");
			 }
			 for(int k=1;k<=space;k++) {
				 System.out.print(" ");
			 }
		 }
		star--;
		space++;
		System.out.println();

	}

}
