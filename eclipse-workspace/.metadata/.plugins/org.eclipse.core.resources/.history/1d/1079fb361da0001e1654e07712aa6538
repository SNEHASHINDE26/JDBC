
package apna;
import java.util.*;
import java.util.Scanner;




public class alpha {

	public static void main(String[] args) {
		
//		Scanner sc = new Scanner(System.in);
		
		for(int line=4; line<=4; line--) {
            for(int star=1;star<=line;star++)		{
            	System.out.print("*");
			}
            System.out.println(" ");
		}
	}
}


